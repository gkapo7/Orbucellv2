{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "cleanUrls": true,
  "functions": {
    "api/**": {
      "runtime": "nodejs20.x",
      "memory": 256,
      "maxDuration": 10
    }
  },
  "rewrites": [
    { 
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}

